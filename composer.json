		"mollie/mollie-api-php": "^2.0",
		"sumup/sumup-ecom-php-sdk": "^1.0",
		"ramsey/uuid": "^3.8",
		"ramsey/uuid-doctrine": "^1.5",
		"stof/doctrine-extensions-bundle": "^1.3",
		"symfony/translation": "5.1.*",
		"twig/extensions": "^1.5",
		"twig/twig": "2.13.*"
	},
	"require-dev" : {
		"cache/integration-tests" : "dev-master",
		"doctrine/cache" : "~1.6",
